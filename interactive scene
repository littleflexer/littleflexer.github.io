

function setup() {
    createCanvas(400, 400);
    background(255);
}

function draw() {
  fill(0,120,123);
  if (mouseIsPressed && keyIsPressed && key === 'r') {

rect(mouseX, mouseY, 50, 50);
  }
if (mouseIsPressed && keyIsPressed && key === 'e') {

ellipse(mouseX, mouseY, 50, 50);
  }
}
function keyTyped(){
  if (key === 'w'){
    background(255);
  }
  else if (key === "b"){
    background(0);
  }
}
